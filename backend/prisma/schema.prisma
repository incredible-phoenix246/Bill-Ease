generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model user {
  id        Int     @id @default(autoincrement())
  firtsName String
  lastName  String
  email     String  @unique
  username  String
  verified  Boolean @default(false)
  password  String

  @@map("Users")
}
